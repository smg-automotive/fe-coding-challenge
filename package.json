{
  "name": "coding-challenge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch next.config.js --exec next dev",
    "build": "CONFIG_ENV=build next build",
    "start": "NODE_OPTIONS='-r next-logger' next start",
    "lint": "eslint --ext ts,js,tsx,jsx .",
    "format": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "typecheck": "npm run typecheck:project && npm run typecheck:cypress",
    "typecheck:project": "tsc",
    "typecheck:cypress": "tsc --project cypress/tsconfig.json",
    "postinstall": "components copy-fonts --path=public"
  },
  "dependencies": {
    "@smg-automotive/components": "22.36.1",
    "@types/jest": "29.5.13",
    "@types/node": "20.9.0",
    "@types/react": "18.3.8",
    "@types/react-dom": "18.3.0",
    "dd-trace": "5.22.0",
    "next": "14.2.13",
    "next-logger": "3.0.2",
    "pino": "8.16.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "swr": "2.2.5",
    "typescript": "5.6.2"
  },
  "devDependencies": {
    "@smg-automotive/eslint-config": "4.0.86",
    "@testing-library/cypress": "10.0.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@types/mocha": "10.0.8",
    "cypress": "13.14.2",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-fix-undefined": "0.1.2",
    "nodemon": "3.1.7",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2"
  }
}
